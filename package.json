{
  "name": "license-info-webpack-plugin",
  "version": "0.7.1",
  "description": "Making a list of package's LICENSE information for webpack",
  "main": "./lib/index.js",
  "files": [
    "lib"
  ],
  "scripts": {
    "watch": "babel -w src -d lib",
    "build": "NODE_ENV=production babel src -d lib",
    "eslint": "eslint src",
    "prettier": "prettier --write '{src,__tests__}/**/*.js'",
    "test": "jest",
    "version": "npm run build"
  },
  "author": "Takahiro Abe",
  "license": "MIT",
  "repository": {
    "type": "git",
    "url": "https://github.com/yami-beta/license-info-webpack-plugin.git"
  },
  "keywords": [
    "webpack",
    "license",
    "plugin"
  ],
  "peerDependencies": {
    "webpack": "^3.0"
  },
  "devDependencies": {
    "babel-cli": "^6.26.0",
    "babel-core": "^6.0.0",
    "babel-jest": "^21.2.0",
    "babel-preset-env": "^1.1.8",
    "jest": "^21.2.1",
    "prettier": "^1.9.2"
  },
  "dependencies": {
    "glob": "^7.1.1",
    "webpack-sources": "^0.1.4"
  }
}
